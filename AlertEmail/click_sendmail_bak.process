    <pd:activity name="SendMail">
        <pd:type>com.tibco.plugin.mail.MailPubActivity</pd:type>
        <pd:resourceType>ae.activities.MailActivityResource</pd:resourceType>
        <pd:x>189</pd:x>
        <pd:y>60</pd:y>
        <config>
            <newMimeSupport>true</newMimeSupport>
            <inputOutputVersion>5.2.0</inputOutputVersion>
            <useSsl>false</useSsl>
            <authenticate>true</authenticate>
            <username>robertwu@91mobcastle.com</username>
            <host>smtp.mxhichina.com:80</host>
            <password>Tibco123</password>
            <ns0:ssl xmlns:ns0="http://www.tibco.com/xmlns/aemeta/services/2002">
                <ns0:strongCipherSuitesOnly>true</ns0:strongCipherSuitesOnly>
                <ns0:cert isRef="true">/Level3Server/.folder</ns0:cert>
            </ns0:ssl>
            <InputHeaders>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </InputHeaders>
        </config>
        <pd:inputBindings>
            <ns:mailActivityInput>
                <from>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/from"/>
                </from>
                <to>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/to"/>
                </to>
                <subject>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/subject"/>
                </subject>
                <pfx:bodyElement>
                    <bodyText>
                        <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/body"/>
                    </bodyText>
                </pfx:bodyElement>
            </ns:mailActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>313</pd:x>
        <pd:y>62</pd:y>
        <config>
            <ConfirmEvent>JMS Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="JMS Queue Sender">
        <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
        <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
        <pd:x>179</pd:x>
        <pd:y>156</pd:y>
        <config>
            <PermittedMessageType>XML Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>click_sendmail</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/AlertEmail/JMS Connection.sharedjmscon</ConnectionReference>
            <InDataxsdString>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="from" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="to" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="subject" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="body" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </InDataxsdString>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInput>
                <Body>
                    <root>
                        <from>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/from"/>
                        </from>
                        <to>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/to"/>
                        </to>
                        <subject>
                            <xsl:value-of select="concat(&quot;Ubuntu Click Error Again While Sending Email : &quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/subject)"/>
                        </subject>
                        <body>
                            <xsl:value-of select="concat(tib:render-xml($Catch),&quot; &#xA;original email body : &#xA;&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/root/body&#xA;)"/>
                        </body>
                    </root>
                </Body>
            </ns1:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>64</pd:x>
        <pd:y>161</pd:y>
        <pd:handler>true</pd:handler>
