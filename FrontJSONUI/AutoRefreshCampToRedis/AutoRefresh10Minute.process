<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>FrontJSONUI/AutoRefreshCampToRedis/AutoRefresh10Minute.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>128</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1483004328495</pd:targetNamespace>
    <pd:activity name="FreshRedis">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>275</pd:x>
        <pd:y>62</pd:y>
        <config>
            <processName>/FrontJSONUI/AutoRefreshCampToRedis/AutoRefreshCampToRedis.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <root>
                <allupdate>
                    <xsl:value-of select="1"/>
                </allupdate>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SendAlertMail">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>351</pd:x>
        <pd:y>152</pd:y>
        <config>
            <processName>/AlertEmail/SendAlertMail.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <MailStart>
                <subject>
                    <xsl:value-of select="&quot;15 Minutes Redis Campaign Auto Refresh Fails&quot;"/>
                </subject>
                <content>
                    <xsl:value-of select="concat(&#xA;&quot; redis fresh campaign every 10 minutes failed :  &quot;,&#xA;$Catch/exceptiondata/*&#xA;)"/>
                </content>
            </MailStart>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>180</pd:x>
        <pd:y>149</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>FreshRedis</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>SendAlertMail</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendAlertMail</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>FreshRedis</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>